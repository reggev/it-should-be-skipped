{
  "name": "@reggev/eslint-plugin-it-should-be-skipped",
  "version": "1.0.0",
  "description": "Ensuring All tests that should be skipped are skipped",
  "repository": {
    "type": "git",
    "url": "https://github.com/reggev/it-should-be-skipped.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Reggev",
  "main": "lib/index.js",
  "scripts": {
    "test:eslint": "eslint ./",
    "test:typescript": "tsc --noEmit --emitDeclarationOnly false",
    "test:rule-tester": "mocha tests --recursive",
    "test": "concurrently -r 'npm:test:*'",
    "pre-commit": "npm t",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "concurrently": "^6.2.0",
    "eslint": "^7.31.0",
    "precommit-hook": "^3.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-jsdoc": "^0.3.23",
    "requireindex": "~1.2.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "@types/estree": "^0.0.50",
    "@types/node": "^16.4.7",
    "@types/requireindex": "^1.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-plugin": "^3.5.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "ISC"
}
